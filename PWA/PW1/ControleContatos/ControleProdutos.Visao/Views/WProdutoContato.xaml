<Window x:Class="ControleProdutos.Visao.Views.WProdutoInsumo"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ControleProdutos.Visao.Views"
        mc:Ignorable="d"
        Title="WProdutoInsumo" Height="450" Width="800">
    <StackPanel>
        <Label>Insumo</Label>
        <TextBox Text="{Binding ObjetoSelecionado.Insumo}" />
        <Label>Quantidade</Label>
        <TextBox Text="{Binding ObjetoSelecionado.Qtd}" />
        <Button Width="100" Command="{Binding Salvar}">Incluir</Button>
    </StackPanel>
</Window>
